<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.tain.mybatis.mappers.BrwMapper">

	<!-- id: selectAll -->
	<select id="selectAll" resultType="Brw">
		select
			id
			, ip_addr
			, user_id
			, create_date
		from
			tb_brw
		where 1=1
	</select>
	
	<!-- id: selectOne -->
	<select id="selectOne" resultType="Brw">
		select
			id
			, ip_addr
			, user_id
			, create_date
		from
			tb_brw
		where 1=1
			and id = #{id}
	</select>
	
</mapper>
